<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE aspectran PUBLIC "-//ASPECTRAN//DTD Aspectran Configuration 6.0//EN"
        "http://aspectran.github.io/dtd/aspectran-6.dtd">
<aspectran>

    <translet name="/">
        <echo id="page">
            <item name="include">logtail/viewer</item>
            <item name="style">fluid plate compact</item>
            <item name="token">#{class:com.aspectran.core.util.security.TimeLimitedPBTokenIssuer^token}</item>
        </echo>
        <dispatch name="templates/frame"/>
    </translet>

</aspectran>